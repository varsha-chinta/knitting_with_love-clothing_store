        <script>
            document.addEventListener("DOMContentLoaded", function() {
                const filterItems = document.querySelectorAll(".new-p-heading li");
                const productBoxes = document.querySelectorAll(".new-product-box");
        
                filterItems.forEach(function(item) {
                    item.addEventListener("click", function() {
                        // Remove 'active' class from all filter items
                        filterItems.forEach(function(filterItem) {
                            filterItem.classList.remove("active");
                        });
        
                        // Add 'active' class to the clicked filter item
                        this.classList.add("active");
        
                        const filterValue = this.getAttribute("data-filter");
        
                        productBoxes.forEach(function(box) {
                            if (filterValue === "all" || box.classList.contains(filterValue)) {
                                box.style.display = "block";
                            } else {
                                box.style.display = "none";
                            }
                        });
                    });
                });
            });
        </script>
        
        
